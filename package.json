{
  "name": "mycli",
  "version": "0.0.0",
  "description": "test cli tool",
  "author": "13Colours",
  "bin": {
    "mycli": "./bin/run"
  },
  "homepage": "https://github.com/13colours/mycli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "13colours/mycli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.8.1",
    "@oclif/plugin-commands": "^2.1.0",
    "@oclif/plugin-help": "^5.2.6",
    "@oclif/plugin-plugins": "^2.1.0",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.34",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11.1.0",
    "mocha": "^9.2.2",
    "oclif": "^3.0.1",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "oclif": {
    "bin": "mycli",
    "dirname": "mycli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Manage your configuration"
      },
      "transer": {
        "description": "Transfer data between systems"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/13colours/mycli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
